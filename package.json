{
  "name": "deathmapnyc",
  "version": "2.0.0",
  "description": "a map of death in nyc",
  "main": "webpack.config.js",
  "scripts": {
    "jupyter": "docker run -it  -v $PWD/data:/home/jovyan/work --rm -p 8888:8888 jupyter/datascience-notebook",
    "start": "webpack-dev-server",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.production.js && echo \"deathmapnyc.com\" > dist/CNAME",
    "deploy-gh-pages": "git subtree push --prefix dist origin gh-pages",
    "deploy-gh-pages-force": "git push origin `git subtree split --prefix dist master`:gh-pages --force",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zischwartz/deathmapnyc.git"
  },
  "keywords": [
    "map",
    "death",
    "nyc"
  ],
  "author": "Zach Schwartz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zischwartz/deathmapnyc/issues"
  },
  "homepage": "https://github.com/zischwartz/deathmapnyc#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.4",
    "dsv-loader": "^2.0.0",
    "file-loader": "^0.11.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "d3": "^4.9.1",
    "html-webpack-plugin": "^2.29.0",
    "mapbox-gl": "^0.38.0",
    "module-to-cdn": "github:zischwartz/module-to-cdn",
    "modules-cdn-webpack-plugin": "^3.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0"
  }
}
